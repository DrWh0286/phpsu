{
  "name": "phpsu/phpsu",
  "license": "MIT",
  "description": "Synchronisation Utility: File and Database",
  "type": "library",
  "keywords": [
    "phpsu",
    "rsync",
    "mysql",
    "mysqldump"
  ],
  "homepage": "https://phpsu.de/",
  "config": {
    "platform": {
      "php": "7.1.3"
    },
    "optimize-autoloader": true,
    "process-timeout": 0
  },
  "authors": [
    {
      "name": "Matthias Vogel",
      "email": "git@kanti.de",
      "role": "Developer"
    },
    {
      "name": "Christian Benthake",
      "email": "git@cben.co",
      "role": "Developer"
    }
  ],
  "bin": [
    "phpsu"
  ],
  "scripts": {
    "test": "vendor/bin/phpunit -c tests/phpunit.xml --testdox --color=always",
    "test:watch": "vendor/bin/phpunit-watcher watch -c tests/phpunit.xml --testdox"
  },
  "autoload": {
    "psr-4": {
      "PHPSu\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPSu\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "symfony/console": "^4.2.0",
    "symfony/process": "^4.2.0",
    "php": ">=7.1.3 <7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*",
    "pluswerk/grumphp-config": "2.*",
    "spatie/phpunit-watcher": "1.*"
  },
  "extra": {
    "pluswerk/grumphp-config": {
      "auto-setting": true
    },
    "grumphp": {
      "config-default-path": "vendor/pluswerk/grumphp-config/grumphp.yml"
    }
  }
}
